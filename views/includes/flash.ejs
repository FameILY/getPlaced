<% errors.forEach(function(message) { %>
    <!-- <div class="toast-container position-fixed bottom-0 end-0 p-3" style="z-index: 1;"> -->
        <div class="alert alert-danger alert-dismissible fade show text-center position-fixed bottom-0 end-0" style="z-index: 1; border-radius: 1rem;">
            <%= message %>
                <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
        </div>
    <!-- </div> -->
    <% }) %>

        <% success.forEach(function(message) { %>
            <div class="alert alert-success alert-dismissible fade show text-center">
                <%= message %>
                    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
            </div>
            <% }) %>